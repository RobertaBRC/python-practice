     },
     "execution_count": 1,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def divisorSum(n):\n",
    "        divisors = []\n",
    "        for i in range(n, 0, -1):\n",
    "            if n % i == 0:\n",
    "                divisors.append(i)\n",
    "        return(sum(divisors))\n",
    "\n",
    "# Testing the newly created function:\n",
    "divisorSum(6)"
   ]
  }
