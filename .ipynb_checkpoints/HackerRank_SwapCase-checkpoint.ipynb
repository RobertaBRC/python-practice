   "source": [
    "_Function to reverse order and swap case of a given sentence_"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "2447f6a0",
   "metadata": {},
   "outputs": [],
   "source": [
    "def reverse_words_order_and_swap_cases(sentence):\n",
    "    x = sentence[::-1]\n",
    "    print(x[::-1].swapcase())\n",
    "    \n",
    "if __name__ == '__main__':\n",
    "    sentence = input()\n",
    "    result = reverse_words_order_and_swap_cases(sentence)"
   ]
  }
